package com.company;

public class test {
	public static void main(String[] args) {
		int plusSum=0;
		for(int i=1;i<=100;i++) {
			plusSum+=i;
		}
		System.out.println("1부터 100까지의 합: "+plusSum);
		
		int xSum=1;
		for(int i=1;i<=100;i++) {
			xSum=xSum*i;
		}
		System.out.println("1부터 100까지의 곱: "+xSum);
	}
}
